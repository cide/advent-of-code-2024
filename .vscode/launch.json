    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "App",
            "request": "launch",
            "mainClass": "aoc.App",
            "projectName": "advent-of-code-2024",
            "args": [
                "${input:day}",
                "${input:part}"
            ]
        }
    ],
    "inputs": [
        {
            "id": "day",
            "type":"promptString",
            "description": "Enter day e.g. '1' ",
            "default": ""
        },
        {
            "id": "part",
            "type":"promptString",
            "description": "Enter part 1 or 2 ",
            "default": "1"
        }
    ]
}